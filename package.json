{
  "name": "vite-plugin-imp",
  "version": "2.2.0",
  "description": "A vite plugin for import style automatic",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "rm -rf dist && tsc -p .",
    "prepublishOnly": "yarn unbuild",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest ./src",
    "patch-cjs": "ts-node scripts/patchCJS.ts --module=CommonJS",
    "unbuild": "unbuild && yarn patch-cjs"
  },
  "keywords": [
    "vite plugin imp"
  ],
  "author": "psaren",
  "license": "MIT",
  "peerDependencies": {
    "vite": ">= 2.0.0-beta.5"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onebay/vite-plugin-imp.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/babel__core": "^7.1.12",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.17",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.1.3",
    "unbuild": "^0.7.4",
    "vite": "2.5.0-beta.3"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/generator": "^7.12.11",
    "@babel/parser": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "chalk": "^4.1.0",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && npm test",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
